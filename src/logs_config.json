{
    "log_folders": [
        "/var/log/secure",
        "/var/log/messages",
        "/var/opt/CARKpsmp/logs",
        "/etc/ssh/sshd_config",
        "/etc/ssh/ssh_config",
        "/etc/ssh/sshd_config.d",
        "/etc/nsswitch.conf",
        "/etc/pam.d/sshd",
        "/etc/pam.d/password-auth",
        "/etc/pam.d/system-auth",
        "/var/tmp/psmp_install.log",
        "/var/opt/CARKpsmp/temp/EnvManager.log",
        "/etc/opt/CARKpsmp/conf/basic_psmpserver.conf",
        "/var/opt/CARKpsmp/temp/PVConfiguration.xml",
        "/var/opt/CARKpsmp/temp/Policies.xml",
        "/etc/os-release",
        "/etc/opt/CARKpsmp/components/components.conf",
        "/etc/opt/CARKpsmpadb/conf/basic_psmpadbridge.conf"
    ],
    "commands": [
        "getent passwd PSMConnect",
        "getent passwd PSMShadowUser",
        "getent group PSMConnectUsers",
        "getent group PSMShadowUsers",
        "getent passwd 50500",
        "getent passwd 50501",
        "getent group 50500",
        "getent group 50501",
        "getent group 50502",
        "ls -lRZa /var/opt/CARKpsmp",
        "ls -lRZa /opt/CARKpsmp",
        "ls -lRZa /etc/opt/CARKpsmp",
        "ls -lRZ /home | grep PSM",
        "ps auxZ | grep psmp",
        "semodule -l",
        "systemctl show | cat",
        "sshd -T",
        "rpm -qa | grep -i CARK",
        "ssh -V",
        "uname -a"
    ],
    "log_categories": {
        "OS": ["secure", "messages", "nsswitch.conf","os-release"],
        "OS/ssh/sshd_config.d": ["/etc/ssh/sshd_config.d"],
        "OS/ssh": ["sshd_config", "ssh_config"],
        "OS/PAM.d": ["sshd", "password-auth", "system-auth"],
        "PSMP": ["/var/opt/CARKpsmp/logs"],
        "PSMP/Installation": ["psmp_install.log", "EnvManager.log"],
        "PSMP/Conf": ["basic_psmpserver.conf", "basic_psmpadbridge.conf","components.conf"],
        "PSMP/Temp": ["PVConfiguration.xml", "Policies.xml"]
    }
}
